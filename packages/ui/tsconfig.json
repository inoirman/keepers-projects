{
	"extends": "@service-suite/tsconfig/base.json",
	"compilerOptions": {
		"jsx": "preserve",
		"lib": ["ES2021", "DOM", "DOM.Iterable"],
		"module": "ESNext",
		"noEmit": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"include": ["src/**/*.ts", "src/**/*.tsx"],
	"exclude": ["node_modules"]
}
